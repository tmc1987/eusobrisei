{
    "version": 2,
    "builds": [
      { "src": "api/app.js", "use": "@vercel/node" },
      { "src": "*.html", "use": "@vercel/static" },
      { "src": "*.css", "use": "@vercel/static" },
      { "src": "*.js", "use": "@vercel/static" },
      { "src": "*.png", "use": "@vercel/static" },
      { "src": "*.jpg", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/calculate_shipping", "dest": "/api/app.js" },
      { "src": "/api/add_to_cart", "dest": "/api/app.js" },
      { "src": "/api/remove_from_cart", "dest": "/api/app.js" },
      { "src": "/api/update_cart_item", "dest": "/api/app.js" },
      { "src": "/api/checkout_items", "dest": "/api/app.js" },
      { "src": "/api/select_freight", "dest": "/api/app.js" },
      { "src": "/api/process_payment", "dest": "/api/app.js" },
      { "src": "/api/emitir_etiqueta", "dest": "/api/app.js" },
      { "src": "/api/test_qr_code", "dest": "/api/app.js" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  